<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
	controllerName="com.saaprojSAADevlopment.controller.CustomerInforamation" xmlns:html="http://www.w3.org/1999/xhtml">
	<App backgroundImage="{dataModel>/images/60}">
		<pages>
			<Page showHeader="false">
				<content>
					<mvc:XMLView viewName="com.saaprojSAADevlopment.view.Navbar7"/>
					<html:div class="row">
						<html:div class="col-md-10 col-lg-10">
							<Text text="{i18n>lblCustomerInformatiom}" class="solutiontitle1 whiteColor"></Text>
							<Input class="cusomerInput" placeholder="Email" type="Email"  value="{GData>/GloBalJson/emailAddress}" id="idEmailInput"></Input>
							<Input  placeholder="{i18n>phoneNumbers}" class="cusomerInput2" type="Number" maxLength="10" value="{GData>/GloBalJson/telephoneNumber}" id="idPhoneNumber"></Input>
								<Button text="{i18n>buttonSend}" class=" sendButton2 sendButton" press="sendEmail"></Button>
						</html:div>
					</html:div>
				</content>
			</Page>
		</pages>
	</App>
</mvc:View>
					<div class="container saaHome fixed-bottom" style="">
						<div class="row h-100" style="margin: -65px 0px 0px 0px;height:105px">
							<div class="col-sm-7 my-auto">
								<m:Text text="{i18n>lblHomeTitle}" class="bold saaTitle"/>
								<br/>
								<m:Text text="{i18n>lblHomeSubTitle}" class="saaText"/>
							</div>
							<div class="col-sm-5 my-auto pull-right text-right">
